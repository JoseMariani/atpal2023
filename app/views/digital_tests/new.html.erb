<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3 class="text-center">
        Add new Test
      </h3>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for(@digital_test, :url => digital_tests_path, :html => {:validate => true, :multipart => true}) do |f| %>

          <%= render(:partial => "form", :locals => {:f => f})%>

          <%= submit_tag("Add test", :class => 'btn btn-success')%>

      <% end %>
    </div>
  </div>
</div>