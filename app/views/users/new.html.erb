<div class="container">
  <div class="col-xs-12 post-index-title">
    <h3 class="text-center">Create User</h3>
  </div>

  <div class="col-xs-12 user_create">
    <div class="col-xs-10">
      <%= form_for(:user, :url => users_path) do |f| %>

          <%= render(:partial => "form", :locals => {:f => f})%>

          <div class="form-buttons pad">
            <%= submit_tag("Add user", :class => 'btn btn-success')%>
          </div>
      <% end %>
    </div>
  </div>
</div>
