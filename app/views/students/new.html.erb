<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h3 class="text-center">
				Add New Student
			</h3>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12 margin-bottom">
			<%= form_for @student, :url => students_path, :html => {:multipart => true} do |f| %>
			
				<%= render(:partial => "form", :locals => {:f => f})%>
				
				<%= submit_tag("Add Student", :class => 'btn btn-success', :id=>'new_student_submit')%>
				
			<% end %>
		</div>
	</div>
</div>